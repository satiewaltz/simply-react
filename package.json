{
  "name": "simply-react",
  "version": "0.0.2",
  "main": "index.js",
  "author": "Dave Barthly <davebarthly@gmail.com>",
  "license": "MIT",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "format": "prettier --write \"src/**/*.{js,jsx}\"",
    "precommit": "lint-staged",
    "start": "webpack-dashboard -- webpack-dev-server",
    "analyze": "yarn build && webpack-bundle-analyzer dist/stats.json",
    "build": "rm -rf /dist && mkdir dist && webpack --config ./webpack.production.config.js --progress --display-error-details --json > dist/stats.json",
    "setup:new": "rm -rf .git README.md && yarn install && git init && touch README.md && echo \"# New Project\n\nBe sure to update this file and package.json with your new project's name.\" > README.md"
  },
  "jest": {
    "moduleFileExtensions": [ "js", "jsx" ],
    "moduleDirectories": [ "node_modules" ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|scss|sass)$": "identity-obj-proxy"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --fix",
      "prettier --write",
      "jest",
      "git add"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^7.1.1",
    "babel-core": "^6.25.0",
    "babel-eslint": "^8.0.1",
    "babel-jest": "^21.2.0",
    "babel-loader": "^7.0.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-class-properties": "^6.23.0",
    "babel-plugin-transform-es2015-destructuring": "^6.23.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.23.0",
    "case-sensitive-paths-webpack-plugin": "^2.1.1",
    "css-hot-loader": "^1.3.1",
    "css-loader": "^0.28.2",
    "directory-named-webpack-plugin": "^2.1.0",
    "eslint": "^4.4.1",
    "eslint-config-prettier": "^2.3.0",
    "eslint-plugin-prettier": "^2.1.2",
    "eslint-plugin-react": "^7.0.1",
    "extract-text-webpack-plugin": "^3.0.1",
    "file-loader": "^0.11.1",
    "html-webpack-plugin": "^2.30.1",
    "husky": "^0.14.3",
    "identity-obj-proxy": "^3.0.0",
    "imports-loader": "^0.7.1",
    "jest": "^21.2.1",
    "lint-staged": "^4.0.3",
    "node-sass": "^4.5.3",
    "path": "^0.12.7",
    "postcss-loader": "^2.0.5",
    "prettier": "^1.5.3",
    "react-hot-loader": "^3.0.0-beta.7",
    "react-test-renderer": "^16.0.0",
    "sass-loader": "^6.0.5",
    "script-ext-html-webpack-plugin": "^1.8.1",
    "style-loader": "^0.18.1",
    "url-loader": "^0.5.9",
    "webpack": "^3.5.2",
    "webpack-bundle-analyzer": "^2.9.0",
    "webpack-dashboard": "^0.4.0",
    "webpack-dev-server": "^2.7.1"
  },
  "dependencies": {
    "normalize.css": "^7.0.0",
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-transition-group": "^2.2.1"
  }
}
